{
  "plugins": ["mongodb", "jest", "no-secrets"],
  "env": {
    "node": true,
    "jest": true,
    "mongo": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:import/recommended" ,"plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "no-console": 0,
    "no-secrets/no-secrets": "error"
  },
  "prettier/prettier": ["error", {}, {
    "usePrettierrc": false
  }]
}
